{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Serve Jekyll Site",
      "type": "shell",
      "request": "launch",
      "command": "bundle exec jekyll serve --livereload",
      "cwd": "${workspaceFolder}",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "problemMatcher": []
    }
  ]
}
